{
  "name": "claude-chatgpt-mcp",
  "version": "1.1.0",
  "main": "dist/index.js",
  "type": "module",
  "description": "A Claude MCP tool to interact with the ChatGPT desktop app on macOS",
  "author": "Syed Azhar",
  "license": "MIT",
  "bin": {
    "claude-chatgpt-mcp": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syedazharmbnr1/claude-chatgpt-mcp.git"
  },
  "keywords": [
    "mcp",
    "claude",
    "chatgpt",
    "mac"
  ],
  "scripts": {
    "dev": "bun run index.ts",
    "build": "node ./node_modules/typescript/bin/tsc",
    "check:stdout": "node scripts/check-stdout.mjs",
    "test": "npm run check:stdout && node ./node_modules/vitest/vitest.mjs run --config vitest.config.ts",
    "test:watch": "node ./node_modules/vitest/vitest.mjs --config vitest.config.ts",
    "coverage": "node ./node_modules/vitest/vitest.mjs run --config vitest.config.ts --coverage",
    "prepare": "npm run build",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@jxa/global-type": "^1.3.6",
    "@jxa/run": "^1.3.6",
    "@modelcontextprotocol/sdk": "^1.5.0",
    "express": "^4.21.2",
    "nanoid": "^5.1.6",
    "pino": "^9.6.0",
    "pino-pretty": "^13.1.1",
    "run-applescript": "^7.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.4",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.1",
    "typescript": "^5.4.2",
    "vitest": "^2.1.9",
    "@vitest/coverage-v8": "^2.1.9"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  }
}
